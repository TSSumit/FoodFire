px;\r\n    height: 115px;\r\n    margin: 10px 10px 10px 14px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    padding-bottom: 32px;\r\n    border-bottom: 0.5px solid #ddd;\r\n    position: relative;\r\n    border-bottom: 12px solid transparent; /* Initial transparent border */\r\n    animation: shimmerBorder 1.5s infinite linear;\r\n  }\r\n  \r\n  .shimmer-img {\r\n    width: 171px;\r\n    height: 100%;\r\n    margin-right: 20px; /* Adjust the margin between image and details */\r\n    border-radius: 8px;\r\n  }\r\n  \r\n  .restaurant-summary-details {\r\n    flex: 1;\r\n  }\r\n  \r\n  .rating-block-shimmer {\r\n    width: 62px;\r\n    height: 66px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    margin-left: 20px; /* Adjust the margin between details and rating block */\r\n    position: relative;\r\n    border-radius: 8px;\r\n    border: 2px solid transparent; /* Initial transparent border */\r\n    animation: shimmerBorder 1.5s infinite linear;\r\n  }\r\n  \r\n  .shimmer-w15{\r\n    width: 15vw;\r\n    height: 18px; \r\n    margin-bottom: 8px; \r\n    background-color: #ddd; \r\n    border-radius: 4px;\r\n  }\r\n  .shimmer-w18{\r\n    width: 18vw;\r\n    height: 18px; \r\n    margin-bottom: 19px; \r\n    background-color: #ddd; \r\n    border-radius: 4px;\r\n  }\r\n  .shimmer-w20{\r\n    width: 20vw;\r\n    height: 18px; \r\n    margin-bottom: 8px; \r\n    background-color: #ddd; \r\n    border-radius: 4px;\r\n  }\r\n  .shimmer-w22{\r\n    width: 22vw;\r\n    height: 18px; \r\n    margin-bottom: 8px; \r\n    background-color: #ddd; \r\n    border-radius: 4px;\r\n    \r\n  }\r\n  \r\n  .rating-stars-shimmer, .rating-count-shimmer {\r\n    width: 90%;\r\n    height: 22px; /* Adjust the height as needed */\r\n    margin: 7px; /* Adjust the vertical spacing between elements */\r\n    background-color: #ddd; /* Shimmer effect color */\r\n    border-radius: 6px; /* Adjust as needed */\r\n  }\r\n  \r\n.restaurant-offer-block{\r\n    margin: 1.5rem 4px;\r\n    width: 780px;\r\n    display: flex;\r\n    justify-content: space-evenly;\r\n    padding-bottom: 32px;\r\n    border-bottom: 0.5px solid #ddd;\r\n}\r\n\r\n\r\n.restaurant-offer{\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-evenly;\r\n    width: 180px;\r\n    height: 60px;\r\n    position: relative;\r\n    border-radius: 8px;\r\n    border: 2px solid transparent; /* Initial transparent border */\r\n    animation: shimmerBorder 1.5s infinite linear;\r\n}\r\n\r\n.offer-code-offer-amount, .offer-logo-offer-title{\r\n    display: flex;\r\n    justify-content: space-evenly;\r\n}\r\n\r\n.offer-logo-shimmer{\r\n    height: 18px;\r\n    width: 18px;\r\n    border-radius: 7px;\r\n} \r\n.offer-title-shimmer{\r\n    height: 18px;\r\n    width: 130px;\r\n    border-radius: 5px;\r\n} \r\n\r\n.offer-code-shimmer{\r\n    height: 16px;\r\n    width: 80px;\r\n    border-radius: 5px;\r\n} \r\n\r\n.offer-amount-shimmer{\r\n    height: 16px;\r\n    width: 70px;\r\n    border-radius: 5px;\r\n}\r\n\r\n.shimmer-menu-card{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 760px;\r\n    height: 112px;\r\n    padding-bottom: 10px;\r\n    margin-bottom: 10px;\r\n    border-bottom: 1.5px solid transparent; /* Initial transparent border */\r\n    animation: shimmerBorder 1.5s infinite linear;\r\n}\r\n\r\n.shimmer-item-details{\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: flex-start;\r\n}\r\n\r\n.menue-card-title{\r\n    height: 22px;\r\n    width: 200px;\r\n    margin-top: 7px;\r\n}\r\n\r\n.menue-card-price{\r\n    height: 21px;\r\n    width: 90px;\r\n    margin-top: 7px;\r\n}\r\n\r\n.menue-card-detail{\r\n    height: 17px;\r\n    width: 340px;\r\n    margin-top: 7px;\r\n}\r\n\r\n.menue-card-img{\r\n    width: 118px;\r\n    height: 99px;\r\n    margin-top: 8px;\r\n}\r\n\r\n\r\n  .stroke {\r\n    /* Add